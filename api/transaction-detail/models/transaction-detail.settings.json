{
  "kind": "collectionType",
  "collectionName": "transaction_details",
  "info": {
    "name": "Transaction_detail"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "quantity": {
      "type": "integer",
      "required": true,
      "default": 1
    },
    "book": {
      "via": "transaction_details",
      "model": "books"
    },
    "transaction": {
      "model": "transaction",
      "via": "transaction_details"
    }
  }
}
